{
  "name": "@barco/saml-protocol",
  "version": "0.5.1",
  "publishConfig": {
    "registry": "https://bin.barco.com/artifactory/api/npm/npm-local"
  },
  "description": "A SAML protocol implementation for SPs and IDPs without opinionated framework bindings",
  "author": "Social Tables (https://www.socialtables.com)",
  "main": "./dist/index.js",
  "keywords": [
    "saml",
    "authentication",
    "sso"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/socialtables/saml-protocol.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "build": "rm -rf dist/** && babel lib --out-dir dist --ignore *.spec.js,testHelpers/*,examples/*",
    "lint": "eslint lib",
    "prepare": "npm run lint && npm test && npm run build",
    "test": "mocha --compilers js:babel-core/register --recursive \"test/**/*.js\"",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "moment": "^2.20.1",
    "node-forge": "0.7.5",
    "xml-crypto": "0.10.1",
    "xml-encryption": "0.11.1",
    "xmlbuilder": "10.0.0",
    "xmldom": "0.1.22",
    "xpath": "0.0.27",
    "xpath.js": "1.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^5.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.8.0",
    "chai": "^4.1.2",
    "mocha": "^5.2.0"
  },
  "bugs": {
    "url": "https://github.com/socialtables/saml-protocol/issues"
  },
  "homepage": "https://github.com/socialtables/saml-protocol#readme",
  "directories": {
    "test": "test"
  }
}
