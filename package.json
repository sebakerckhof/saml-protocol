{
  "name": "@socialtables/saml-protocol",
  "version": "0.3.4",
  "description": "A SAML protocol implementation for SPs and IDPs without opinionated framework bindings",
  "author": "Social Tables (https://www.socialtables.com)",
  "main": "./dist/index.js",
  "keywords": [
    "saml",
    "authentication",
    "sso"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/socialtables/saml-protocol.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=4.0.0"
  },
  "scripts": {
    "build": "rm -rf dist/** && babel lib --out-dir dist --ignore *.spec.js,testHelpers/*,examples/*",
    "lint": "eslint lib",
    "prepare": "npm run lint && npm test && npm run build",
    "test": "mocha --compilers js:babel-core/register --recursive \"test/**/*.js\"",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "moment": "^2.20.1",
    "node-forge": "0.7.1",
    "xml-crypto": "0.10.1",
    "xml-encryption": "0.11.0",
    "xmlbuilder": "9.0.4",
    "xmldom": "0.1.21",
    "xpath": "0.0.27",
    "xpath.js": "1.0.7"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.13.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "chai": "^4.1.2",
    "mocha": "^4.0.1"
  },
  "bugs": {
    "url": "https://github.com/socialtables/saml-protocol/issues"
  },
  "homepage": "https://github.com/socialtables/saml-protocol#readme",
  "directories": {
    "test": "test"
  }
}
