{
  "name": "@barco/saml-protocol",
  "version": "0.5.7",
  "upstreamVersion": "0.3.7",
  "publishConfig": {
    "registry": "https://bin.barco.com/artifactory/api/npm/npm-local"
  },
  "description": "A SAML protocol implementation for SPs and IDPs without opinionated framework bindings",
  "author": "Social Tables (https://www.socialtables.com)",
  "main": "./dist/index.js",
  "keywords": [
    "saml",
    "authentication",
    "sso"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/socialtables/saml-protocol.git"
  },
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "build": "rm -rf dist/** && ./node_modules/.bin/babel lib --out-dir dist --ignore *.spec.js,testHelpers/*,examples/*",
    "lint": "npx eslint lib",
    "prepare": "npm run lint && npm test && npm run build",
    "test": "mocha --require @babel/register --recursive \"test/**/*.js\"",
    "test:watch": "npm test -- --watch"
  },
  "dependencies": {
    "moment": "2.27.0",
    "node-forge": "0.10.0",
    "xml-crypto": "1.5.3",
    "xml-encryption": "1.2.1",
    "xmlbuilder": "15.1.1",
    "xmldom": "0.1.22",
    "xpath": "0.0.29",
    "xpath.js": "1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.11.6",
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/register": "7.11.5",
    "babel-eslint": "10.1.0",
    "chai": "4.2.0",
    "eslint": "7.8.1",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-plugin-import": "2.22.0",
    "mocha": "8.1.3"
  },
  "bugs": {
    "url": "https://github.com/socialtables/saml-protocol/issues"
  },
  "homepage": "https://github.com/socialtables/saml-protocol#readme",
  "directories": {
    "test": "test"
  }
}
